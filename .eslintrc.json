{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "comma-dangle": "off",
    "consistent-return": "off",
    "no-await-in-loop": "off",
    "no-console": "off",
    "no-multiple-empty-lines": "off",
    "no-restricted-syntax": "off",
    "no-use-before-define": "warn",
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "padded-blocks": "off"
  }
}
